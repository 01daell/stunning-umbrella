{
  "name": "studiokit/app",
  "type": "project",
  "description": "StudioKit - Branding kit SaaS",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0",
    "laravel/sanctum": "^3.2",
    "laravel/tinker": "^2.8",
    "laravel/cashier": "^15.0",
    "stripe/stripe-php": "^13.0",
    "barryvdh/laravel-dompdf": "^2.0",
    "intervention/image": "^2.7"
  },
  "require-dev": {
    "fakerphp/faker": "^1.21",
    "laravel/breeze": "^1.29",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.30",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^7.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
